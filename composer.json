{
    "name": "positron48/memorystorage",
    "description": "Classes for storing data in shared memory",
    "type": "library",
    "authors": [
        {
            "name": "Filatov Anton",
            "email": "positron48@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "malkusch/lock": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "MemoryStorage\\": "classes/"
        },
        "files": [
            "classes/ArrayMemoryStorage.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MemoryStorage\\Tests\\": "tests/"
        }
    },
    "license": "GPL-3.0-or-later",
    "require-dev": {
        "phpunit/phpunit": "^9.4|^10.0"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "test-coverage-text": "phpunit --coverage-text",
        "test-coverage-clover": "phpunit --coverage-clover coverage.xml"
    },
    "scripts-descriptions": {
        "test": "Run the PHPUnit test suite",
        "test-coverage": "Run tests with HTML coverage report",
        "test-coverage-text": "Run tests with text coverage report",
        "test-coverage-clover": "Run tests with Clover coverage report"
    }
}
